<!-- Experimental XML-based DispSpec for Kernel Traffic format -->
<dispspec>

	<metadata>
		<_name>&quot;Kernel Traffic&quot; Newsletter</_name>
		<_description>A newletter tracking the technical development of a project within the Free/Open Source Software world</_description>
	</metadata>
	<document-models>
	</document-models>
	<element-list>
			
		<element localName="kc" type="structural" color="a0b0c0">
			<_name>Kernel Cousin Newsletter</_name>
			<header-info xpath="concat(normalize-space(child::title/text()), &quot; #&quot;, normalize-space(child::issue/@num))"></header-info>
		</element>

		<element localName="kcref" type="structural" color="a0b0c0">
		</element>

		<element localName="quote" type="structural" color="a0b0c0"  icon="cong-paragraph"> 
			<_name>Quotation</_name>
			<header-info xpath="normalize-space(@who)"></header-info>
		</element>

		<element localName="section" type="structural" color="a0b0c0" icon="cong-subsection"> 
			<_name>Section</_name>
			<header-info xpath="concat(normalize-space(@title), &quot; (&quot;, normalize-space(@subject), &quot;)&quot;)"></header-info>
		</element>

		<element localName="stats" type="structural" color="a0b0c0"  icon="cong-metadata"> 
			<_name>Statistics</_name>
		</element>

		<element localName="person" type="structural" color="a0b0c0"  icon="cong-author"> 
			<_name>Person</_name>
		</element>

		<element localName="vcelement" type="structural" color="a0b0c0"> 
			<_name>Version Control Activity</_name>
		</element>

		<element localName="vcstats" type="structural" color="a0b0c0"> 
			<_name>Version Control Statistics</_name>
		</element>

		<element localName="vctree" type="structural" color="a0b0c0"> 
			<_name>Version Control Tree</_name>
		</element>

		<element localName="intro" type="structural" color="a0b0c0" icon="cong-subsection"> 
			<_name>Introduction</_name>
		</element>

		<element localName="issue" type="structural" color="a0b0c0" icon="cong-metadata"> 
			<_name>Issue</_name>
		</element>

		<element localName="author" type="structural" color="a0b0c0" icon="cong-author"> 
			<_name>Author</_name>
		</element>

		<element localName="translator" type="structural" color="a0b0c0" icon="cong-author"> 
			<!-- Translators: It is just a word "Translator" that should be translated, not replaced by your initials :) -->
			<_name>Translator</_name>
		</element>

		<element localName="editor" type="structural" color="a0b0c0" icon="cong-author"> 
			<_name>Editor</_name>
		</element>

		<element localName="correction" type="structural" color="a0b0c0" icon="cong-author"> 
			<_name>Correction</_name>
		</element>
		<element localName="editorialize" type="structural" color="a0b0c0" icon="cong-author"> 
			<_name>Editorialize</_name>
		</element>

		<element localName="li" type="structural" color="a0b0c0"  icon="cong-list-item"> 
			<_name>List Item</_name>
		</element>

		<element localName="ol" type="structural" color="a0b0c0"  icon="cong-numbered-list"> 
			<_name>List (ordered)</_name>
		</element>

		<element localName="p" type="plugin" service-id="paragraph" color="a0b0c0"  icon="cong-paragraph"> 
			<_name>Paragraph</_name>
		</element>

		<element localName="pre" type="structural" color="a0b0c0"> 
		</element>

		<element localName="a" type="span" color="a0b0c0"> 
			<_name>Hyperlink</_name>
		</element>

		<element localName="b" type="span" color="a0b0c0"> 
			<_name>Bold</_name>
		</element>

		<element localName="blockquote" type="structural" color="a0b0c0"> 
			<_name>Block Quote</_name>
		</element>

		<element localName="br" type="structural" color="a0b0c0"> 
			<_name>Break (manual)</_name>
		</element>

		<element localName="center" type="structural" color="a0b0c0"> 
			<_name>Centered</_name>
		</element>

		<element localName="cite" type="span" color="a0b0c0"> 
			<_name>Cite</_name>
		</element>

		<element localName="code" type="structural" color="a0b0c0">
			<_name>Code</_name>
		</element>

		<element localName="em" type="span" color="a0b0c0"> 
			<_name>Emphasis</_name>
		</element>

		<element localName="font" type="span" color="a0b0c0"> 
			<_name>Font</_name>
		</element>

		<element localName="h1" type="structural" color="a0b0c0" icon="cong-subsection"> 
			<_name>Heading (level 1)</_name>
		</element>

		<element localName="h2" type="structural" color="a0b0c0" icon="cong-subsection"> 
			<_name>Heading (level 2)</_name>
		</element>

		<element localName="h3" type="structural" color="a0b0c0" icon="cong-subsection"> 
			<_name>Heading (level 3)</_name>
		</element>

		<element localName="headquote" type="structural" color="a0b0c0"> 
		</element>

		<element localName="hr" type="structural" color="a0b0c0"> 
		</element>

		<element localName="i" type="span" color="a0b0c0"> 
			<_name>Italicised</_name>
		</element>
		<element localName="strong" type="span" color="a0b0c0"> 
			<_name>Strong</_name>
		</element>

		<element localName="table" type="structural" color="a0b0c0">
		<!-- FIXME: use table plugin -->
		</element>

		<element localName="td" type="structural" color="a0b0c0">
		<!-- FIXME: use table plugin -->
		</element>

		<element localName="title" type="structural" color="a0b0c0" icon="cong-title">
			<_name>Title</_name>
		</element>


		<element localName="topic" type="structural" color="a0b0c0" icon="cong-subsection"> 
			<_name>Topic</_name>
		</element>

		<element localName="tr" type="structural" color="a0b0c0">
		<!-- FIXME: use table plugin -->
		</element>

		<element localName="tt" type="structural" color="a0b0c0">
		</element>

		<element localName="u" type="span" color="a0b0c0"> 
			<_name>Underlined</_name>
		</element>

		<element localName="ul" type="structural" color="a0b0c0"  icon="cong-bulleted-list"> 
			<_name>List (unordered)</_name>
		</element>

	</element-list>

</dispspec>

<?xml version="1.0"?>
<!-- Experimental DispSpec for Webpages using Norman Walsh's "website" format  -->
<dispspec>

	<metadata>
		<_name>Webpage</_name>
		<_description></_description>
	</metadata>
	
	<!-- probably should specify the DTD that this annotates; hence we can get at the DTD if none was supplied in the document -->
	<document-models/>

	<element-list>
			
		<!-- The high-level divisions -->
		<element localName="webpage" type="structural" color="a0b0c0">
			<_name>Web Page</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="title" type="structural" color="a0b0c0">
			<_name>Title</_name>
		</element>
		<element localName="subtitle" type="structural" color="a0b0c0">
			<_name>Subtitle</_name>
		</element>
		<element localName="authorgroup" type="structural" color="a0b0c0">
			<_name>Authorship Information</_name>
		</element>
		<element localName="author" type="structural" color="a0b0c0">
			<_name>Author</_name>
		</element>
		<element localName="editor" type="structural" color="a0b0c0">
			<_name>Editor</_name>
		</element>
		<element localName="publisher" type="structural" color="a0b0c0">
			<_name>Publisher</_name>
		</element>
		<element localName="publishername" type="structural" color="a0b0c0">
			<_name>Publisher Name</_name>
		</element>
		<element localName="address" type="structural" color="a0b0c0">
			<_name>Address</_name>
		</element>
		<element localName="printhistory" type="structural" color="a0b0c0">
			<_name>Printing History</_name>
		</element>
		<element localName="isbn" type="structural" color="a0b0c0">
			<_name>ISBN</_name>
			<_short-desc>The International Standard Book Number of a document.</_short-desc>
		</element>
		<element localName="issn" type="structural" color="a0b0c0">
			<_name>ISSN</_name>
			<_short-desc>The International Standard Serial Number of a periodical.</_short-desc>
		</element>
		<element localName="releaseinfo" type="structural" color="a0b0c0">
			<_name>Release Information</_name>
			<_short-desc>A brief description of the version of a document.</_short-desc>
		</element>
		<element localName="pubdate" type="structural" color="a0b0c0">
			<_name>Date of Publication</_name>
		</element>
		<element localName="copyright" type="structural" color="a0b0c0">
			<_name>Copyright</_name>
		</element>
		<element localName="year" type="structural" color="a0b0c0">
			<_name>Year</_name>
		</element>
		<element localName="holder" type="structural" color="a0b0c0">
			<_name>Copyright Holder</_name>
		</element>
		<element localName="docinfo" type="structural" color="a0b0c0">
			<_name>Bibliographic Information</_name>
		</element>
		<element localName="abstract" type="structural" color="a0b0c0">
			<_name>Abstract</_name>
			<_short-desc>A summary.</_short-desc>
		</element>
		<element localName="para" type = "structural">
			<_name>Paragraph</_name>
		</element>
		<element localName="legalnotice" type="structural" color="a0b0c0">
			<_name>Legal Notice</_name>
		</element>
		<element localName="colophon" type="structural" color="a0b0c0">
			<_name>Colophon</_name>
		</element>
		<element localName="qandaset" type="structural" color="a0b0c0">
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="qandadiv" type="structural" color="a0b0c0">
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="qandaentry" type="structural" color="a0b0c0">
		</element>
		<element localName="question" type="structural" color="a0b0c0">
		</element>
		<element localName="answer" type="structural" color="a0b0c0">
		</element>
		<element localName="bibliography" type="structural" color="a0b0c0">
			<_name>Bibliography</_name>
		</element>
		<element localName="biblioentry" type="structural" color="a0b0c0">
			<_name>Entry</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="bibliodiv" type="structural" color="a0b0c0">
			<_name>Section</_name>
		</element>
		<element localName="bridgehead" type="structural" color="a0b0c0">
			<_name>Free-floating Heading</_name>
		</element>
		<!-- The various sections -->
		<element localName="preface" type="structural" color="a0b0c0">
			<_name>Preface</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="part" type="structural" color="a0b0c0">
			<_name>Part</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="partintro" type="structural" color="a0b0c0">
			<_name>Part Introduction</_name>
		</element>
		<element localName="chapter" type="structural" color="a0b0c0">
			<_name>Chapter</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="sect1" type="structural" color="a1b0c0">
			<_name>Section</_name>
			<_short-desc>A top-level section of document</_short-desc>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="sect2" type="structural" color="a0b0c0">
			<_name>Subsection</_name>
			<_short-desc>A subsection within a section</_short-desc>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="sect3" type="structural" color="a0b0c0">
			<_name>Subsection (level 3)</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="sect4" type="structural" color="a0b0c0">
			<_name>Subsection (level 4)</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="sect5" type="structural" color="a0b0c0">
			<_name>Section (level 5)</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="reference" type="structural" color="a0b0c0">
			<_name>Reference</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="refentry" type="structural" color="a0b0c0">
			<_name>Reference Page</_name>
			<header-info xpath="normalize-space(child::refname)"/>
		</element>
		<element localName="refmeta" type="structural" color="a0b0c0">
			<_name>Metadata</_name>
			<_short-desc>Meta-information about a Reference Page</_short-desc>
		</element>
		<element localName="refentrytitle" type="structural" color="a0b0c0">
			<_name>Title</_name>
		</element>
		<element localName="refmiscinfo" type="structural" color="a0b0c0">
			<_name>Additional metadata</_name>
		</element>
		<element localName="indexterm" type="structural" color="a0b0c0">
			<_name>Index Term</_name>
		</element>

		<element localName="refnamediv" type="structural" color="a0b0c0">
			<_name>Naming</_name>
			<_short-desc>Name, purpose and classification about a Reference Page</_short-desc>
		</element>
		<element localName="refname" type="structural" color="a0b0c0">
			<_name>Name</_name>
		</element>
		<element localName="refpurpose" type="structural" color="a0b0c0">
			<_name>Purpose</_name>
		</element>
		<element localName="refsynopsisdiv" type="structural" color="a0b0c0">
			<_name>Synopsis (syntax)</_name>
		</element>
		<element localName="refsect1" type="structural" color="a0b0c0">
			<_name>Section</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="refsect2" type="structural" color="a0b0c0">
			<_name>Subsection</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="refsect3" type="structural" color="a0b0c0">
			<_name>Subsubsection</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="appendix" type="structural" color="a0b0c0">
			<_name>Appendix</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="glossary" type="structural" color="a0b0c0">
			<_name>Glossary</_name>
		</element>
		<element localName="glossentry" type="structural" color="a0b0c0">
			<_name>Glossary Entry</_name>
			<header-info xpath="normalize-space(child::glossterm)"/>
		</element>
		<element localName="glossterm" type="structural" color="a0b0c0">
			<_name>Term</_name>
		</element>
		<element localName="glossdef" type="structural" color="a0b0c0">
			<_name>Definition</_name>
		</element>

		<element localName="informalexample" type="structural" color="a0b0c0">
			<_name>Example (informal)</_name>
		</element>
		<element localName="informaltable" type="structural" color="a0b0c0">
			<_name>Table (informal)</_name>
		</element>
		<element localName="simplelist" type="structural" color="a0b0c0">
			<_name>List (simple)</_name>
		</element>
		<!-- List support -->
		<element localName="itemizedlist" type="structural" color="a0b0c0">
			<_name>List (itemised)</_name>
		</element>
		<element localName="orderedlist" type="structural" color="a0b0c0">
			<_name>List (ordered)</_name>
		</element>
		<element localName="listitem" type="structural" color="a0b0c0">
		</element>
		<element localName="member" type="structural" color="a0b0c0">
		</element>
		<element localName="variablelist" type="structural" color="a0b0c0">
		</element>
		<element localName="varlistentry" type="structural" color="a0b0c0">
		</element>
		<!-- Miscellanous stuff -->

		<element localName="figure" type="structural" color="a0b0c0">
		</element>
		<element localName="screenshot" type="structural" color="a0b0c0">
		</element>
		<element localName="informalfigure" type="structural" color="a0b0c0">
		</element>
		<element localName="mediaobject" type="structural" color="a0b0c0">
		</element>
		<element localName="imageobject" type="structural" color="a0b0c0">
		</element>
		<element localName="imagedata" type="embed-external-file" filenameXPath="@fileref" color="a0b0c0">
			<!-- fileref is assumed for now -->
		</element>
		<element localName="textobject" type="structural" color="a0b0c0">
		</element>
		<element localName="anchor" type="structural" color="a0b0c0">
			<!-- really it's just a placemarker; need a new localName type; posisbly should have an icon for this -->
		</element>
		<element localName="caption" type="structural" color="a0b0c0">
		</element>
		<element localName="graphic" type="embed-external-file" filenameXPath="@fileref" color="a0b0c0">
			<!-- fileref is assumed for now -->
		</element>
		<element localName="programlisting" type="structural" color="a0b0c0">
			<!-- 
				perhaps this should use a fixed-space font
				Perhaps we need a boolean to determine whether something
				should appear in the regular proportional spacing or in
				fixed-space? (defaults to proportional).
				Could also do with a verbatim flag which respects carriage returns...
				Even better would be to invoke a Scintilla control and have syntax highlighting!
			-->
			
		</element>
		<element localName="funcsynopsis" type="structural" color="a0b0c0">
			<!-- various related localNames need to be added -->
		</element>
		<element localName="screen" type="structural" color="a0b0c0">
			<_name>Screen</_name>
		</element>
		<element localName="cmdsynopsis" type="structural" color="a0b0c0">
			<_name>Command Synopsis</_name>
		</element>
		<element localName="procedure" type="structural" color="a0b0c0">
			<_name>Procedure</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="step" type="structural" color="a0b0c0">
			<_name>Step</_name>
			<header-info xpath="normalize-space(child::title)"/>
		</element>
		<element localName="substeps" type="structural" color="a0b0c0">
			<_name>Substeps</_name>
		</element>

		<!-- Various inline elements, done as spans follow: -->

		<element localName="acronym" type="span" color="a0b0c0">
			<_name>Acronym</_name>
		</element>
 		<element localName="application" type="span" color="a0b0c0">
			<_name>Application</_name>
		</element>
 		<element localName="bibliomisc" type="span" color="a0b0c0">
			<_name>Miscellaneous Bibliographic Information</_name>
		</element>
		<element localName="citetitle" type="span" color="a0b0c0">
			<_name>Title Citation</_name>
		</element>
		<element localName="city" type="span" color="a0b0c0">
			<_name>City</_name>
		</element>
		<element localName="edition" type="span" color="a0b0c0">
			<_name>Edition</_name>
		</element>
		<element localName="email" type="span" color="a0b0c0">
			<_name>email</_name>
			<_short-desc>An email address</_short-desc>
		</element>
		<element localName="emphasis" type="span" color="a2b0c0">
			<_name>Emphasis</_name>
		</element>
		<element localName="filename" type="span" color="a0b0c0">
			<_name>Filename</_name>
		</element>
		<element localName="firstname" type="span" color="a0b0c0">
			<_name>First Name</_name>
		</element>
		<element localName="firstterm" type="span" color="a0b0c0">
			<_name>First Term </_name>
			<_short-desc>The first place where a word or term occurs in a particular context</_short-desc>
		</element>
		<element localName="function" type="span" color="a0b0c0">
			<_name>Function</_name>
			<_short-desc>The name of a function, subroutine or procedure</_short-desc>
		</element>
		<element localName="literal" type="span" color="a0b0c0">
			<_name>Literal</_name>
			<_short-desc>A specific piece of data from a computer system, to be taken literally.</_short-desc>
		</element>
		<element localName="primary" type="span" color="a0b0c0">
		</element>
		<element localName="productname" type="span" color="a0b0c0">
			<_name>Product Name</_name>
		</element>
		<element localName="replaceable" type="span" color="a0b0c0">
			<_name>Replaceable Text</_name>
		</element>
		<element localName="secondary" type="span" color="a0b0c0">
		</element>
		<element localName="sgmltag" type="span" color="a0b0c0">
			<_name>SGML Tag</_name>
		</element>
		<element localName="structname" type="span" color="a0b0c0">
			<_name>Structure Name</_name>
		</element>
		<element localName="surname" type="span" color="a0b0c0">
			<_name>Surname</_name>
		</element>
		<element localName="symbol" type="span" color="a0b0c0">
			<_name>Symbol</_name>
		</element>
		<element localName="term" type="span" color="a0b0c0">
			<_name>Term</_name>
		</element>
		<element localName="ulink" type="span" color="a0b0c0">
			<_name>URL Link</_name>
			<_short-desc>A URL link (perhaps to a web site)</_short-desc>
		</element>
		<element localName="xref" type="span" color="a0b0c0">
			<_name>Cross-reference</_name>
		</element>

		<element localName="guibutton" type="span" color="a0b0c0">
			<_name>GUI Button</_name>
		</element>
		<element localName="guilabel" type="span" color="a0b0c0">
			<_name>GUI Label</_name>
		</element>
		<element localName="guimenu" type="span" color="a0b0c0">
			<_name>GUI Menu</_name>
		</element>
		<element localName="guisubmenu" type="span" color="a0b0c0">
			<_name>GUI Submenu</_name>
		</element>
		<element localName="guimenuitem" type="span" color="a0b0c0">
			<_name>GUI Menu Item</_name>
		</element>
		<element localName="keycap" type="span" color="a0b0c0">
			<_name>Key Caption</_name>
			<_short-desc>The printed text on a particular key on a keyboard</_short-desc>
		</element>
		<element localName="menuchoice" type="span" color="a0b0c0">
			<_name>Menu Choice</_name>
		</element>
		<!-- etc -->

	</element-list>

</dispspec>

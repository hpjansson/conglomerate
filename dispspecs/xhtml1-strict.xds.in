<?xml version="1.0"?>
<dispspec>
	<metadata>
		<_name>XHTML 1.0 (strict)</_name>
		<_description>The strict XML version of HTML </_description>
	</metadata>
	<serialisation>
		<format extension="html"/>
	</serialisation>
	<document-models>
		<external-document-model type="dtd" public-id="-//W3C//DTD XHTML 1.0 Strict//EN" system-id="http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"/>
	</document-models>
	<element-list nsURI="http://www.w3.org/1999/xhtml">
		<typical-prefix/>
		<typical-prefix prefix="html"/>
		<typical-prefix prefix="xhtml"/>
	
		<!-- Skeletal LocalNames -->
		<element localName="body" type="structural">
			<_name>Body</_name>
		</element>
		<element localName="head" type="structural">
			<_name>Head</_name>
		</element>
		<element localName="html" type="structural">
			<_name>HTML Page</_name>
		</element>

		<!-- Header LocalNames -->
		<element localName="base" type="structural">
			<_name>Base</_name>
		</element>
		<element localName="link" type="structural">
			<_name>Link</_name>
		</element>
		<element localName="meta" type="structural">
			<_name>Metainformation</_name>
		</element>
		<element localName="style" type="structural">
			<_name>Style</_name>
		</element>
		<element localName="title" type="structural">
			<_name>Title</_name>
		</element>

		<!-- Hyperlinks -->
		<element localName="a" type="span">
			<_name>Anchor</_name>
		</element>

		<!-- Text Markup: Physical-based -->
		<element localName="b" type="span">
			<_name>Bold</_name>
		</element>
		<element localName="big" type="span">
			<_name>Big</_name>
		</element>
		<element localName="i" type="span">
			<_name>Italic</_name>
		</element>
		<element localName="small" type="span">
			<_name>Small</_name>
		</element>
		<element localName="sub" type="span">
			<_name>Subscript</_name>
		</element>
		<element localName="tt" type="span">
			<_name>Teletype</_name>
		</element>

		<!-- Text Markup: Content-based -->
		<element localName="cite" type="span">
			<_name>Citation</_name>
		</element>
		<element localName="code" type="span">
			<_name>Code</_name>
		</element>
		<element localName="dfn" type="span">
			<_name>Definition</_name>
		</element>
		<element localName="em" type="span">
			<_name>Emphasis</_name>
		</element>
		<element localName="kbd" type="span">
			<_name>Keyboard</_name>
		</element>
		<element localName="samp" type="span">
			<_name>Sample</_name>
		</element>
		<element localName="strong" type="span">
			<_name>Strong</_name>
		</element>
		<element localName="var" type="span">
			<_name>Variable</_name>
		</element>

		<!-- Lists -->
		<element localName="dl" type="structural">
			<_name>Definition List</_name>
		</element>
		<element localName="li" type="plugin" service-id="listitem">
			<_name>List Item</_name>
		</element>
		<element localName="ol" type="structural">
			<_name>List (ordered)</_name>
		</element>
		<element localName="ul" type="structural">
			<_name>List (unordered)</_name>
		</element>

		<!-- Forms -->
		<element localName="form" type="structural">
			<_name>Interactive Form</_name>
		</element>
		<element localName="input" type="structural">
			<_name>Form Control</_name>
		</element>
		<element localName="option" type="structural">
			<_name>Selectable Choice</_name>
		</element>
		<element localName="select" type="structural">
			<_name>Option Selector</_name>
		</element>
		<element localName="textarea" type="structural">
			<_name>Multi-line Text Field</_name>
		</element>

		<!-- Rules, images and multimedia -->
		<element localName="img" type="structural">
			<_name>Image</_name>
		</element>
		<element localName="map" type="structural">
			<_name>Image Map</_name>
		</element>

		<!-- Tables -->
		<element localName="caption" type="span">
			<_name>Caption</_name>
		</element>
		<element localName="col" type="span">
			<_name>Column</_name>
		</element>
		<element localName="colgroup" type="structural">
			<_name>Column Group</_name>
		</element>
		<element localName="table" type="structural">
			<_name>Table</_name>
		</element>
		<element localName="tbody" type="structural">
			<_name>Table Body</_name>
		</element>
		<element localName="td" type="span">
			<_name>Table Cell (data)</_name>
		</element>
		<element localName="tfoot" type="structural">
			<_name>Table Footer</_name>
		</element>
		<element localName="th" type="span">
			<_name>Table Cell (header)</_name>
		</element>
		<element localName="thead" type="structural">
			<_name>Table Header</_name>
		</element>
		<element localName="tr" type="structural">
			<_name>Table Row</_name>
		</element>

		<!-- Executable Content -->
		<element localName="object" type="structural">
			<_name>Object</_name>
		</element>
		<element localName="script" type="structural">
			<_name>Script</_name>
		</element>

		<!-- Content Presentation and Flow -->
		<element localName="address" type="span">
			<_name>Address</_name>
		</element>
		<element localName="blockquote" type="structural">
			<_name>Block Quotation</_name>
		</element>
		<element localName="br" type="span">
			<_name>Forced Line Break</_name>
		</element>
		<element localName="div" type="span">
			<_name>Division</_name>
		</element>
		<element localName="h1" type="structural">
			<_name>Heading (level 1)</_name>
		</element>
		<element localName="h2" type="structural">
			<_name>Heading (level 2)</_name>
		</element>
		<element localName="h3" type="structural">
			<_name>Heading (level 3)</_name>
		</element>
		<element localName="h4" type="structural">
			<_name>Heading (level 4)</_name>
		</element>
		<element localName="h5" type="structural">
			<_name>Heading (level 5)</_name>
		</element>
		<element localName="h6" type="structural">
			<_name>Heading (level 6)</_name>
		</element>
		<element localName="hr" type="structural">
			<_name>Horizontal Rule</_name>
		</element>
		<element localName="p" type="structural">
			<_name>Paragraph</_name>
		</element>

		<!-- Unclassified localNames -->

		<element localName="param" type="span">
			<_name>Parameter</_name>
		</element>

		<element localName="button" type="span">
			<_name>Button</_name>
		</element>
		<element localName="dt" type="span">
			<_name>Definition Term</_name>
		</element>

		<!-- Ongoing classification -->
		<element localName="area" type="span">
			<_name>Area</_name>
		</element>
		<element localName="dd" type="span">
			<_name>Definition</_name>
		</element>
		<element localName="fieldset" type="span">
			<_name>Form Control Group</_name>
		</element>
		<element localName="ins" type="span">
			<_name>Inserted Text</_name>
		</element>
		<element localName="span" type="span">
			<_name>Span</_name>
		</element>
		<element localName="label" type="span">
			<_name>Label</_name>
		</element>
		<element localName="acronym" type="span">
			<_name>Acronym</_name>
		</element>
		<element localName="noscript" type="structural">
			<_name>Alternative Non-Scripted Content</_name>
		</element>
		<element localName="sup" type="span">
			<_name>Superscript</_name>
		</element>
		<element localName="optgroup" type="structural">
			<_name>Option Group</_name>
		</element>
		<element localName="pre" type="span">
			<_name>Preformatted Text</_name>
		</element>
		<element localName="bdo" type="span">
			<_name>Bidirectional Override</_name>
		</element>
		<element localName="del" type="span">
			<_name>Deleted Text</_name>
		</element>
		<element localName="q" type="span">
			<_name>Inline Quotation</_name>
		</element>
		<element localName="abbr" type="span">
			<_name>Abbreviated Form</_name>
		</element>
		<element localName="legend" type="span">
			<_name>Legend</_name>
		</element>
	</element-list>
</dispspec>

<?xml version="1.0"?>
<dispspec>
	<metadata>
		<_name>TEI Lite</_name>
		<_description>TEI stands for Text Encoding Initiative, see http://www.tei-c.org/ for more information; the TEI Lite dtd is a subset of the powerful TEI dtds which comprises the most commonly used elements.</_description>
	</metadata>
	<document-models>
		<external-document-model type="dtd" public-id="-//TEI P4//DTD Main Document Type//EN" system-id="http://www.tei-c.org/Guidelines/DTD/tei2.dtd"/>
	</document-models>
	<element-list>
		<element type="span" localName="signed" whitespace="normalize">
			<_name>Signature</_name>
			<_short-desc>The closing salutation, etc., appended to a foreword, dedicatory epistle, or other division of a text</_short-desc>
		</element>
		<element type="structural" localName="docTitle" whitespace="normalize">
			<_name>Document Title</_name>
			<_short-desc>The title of a document, including all its constituents, as given on a title page</_short-desc>
		</element>
		<element type="span" localName="xptr" whitespace="normalize">
			<_name>Extended Pointer (xptr)</_name>
			<_short-desc>A pointer to another location in the current document or an external document</_short-desc>
		</element>
		<element type="span" localName="cell" whitespace="normalize">
			<_name>Cell</_name>
			<_short-desc>One cell of a table</_short-desc>
		</element>
		<element type="structural" localName="respStmt" whitespace="normalize">
			<_name>Responsibility Statement (respStmt)</_name>
			<_short-desc>A statement of responsibility for someone responsible for the intellectual content of a text, edition, recording, or series, where the specialized elements for authors, editors, etc. do not suffice or do not apply</_short-desc>
		</element>
		<element type="span" localName="edition" whitespace="normalize">
			<_name>Edition</_name>
			<_short-desc>The particularities of one edition of a text</_short-desc>
		</element>
		<element type="span" localName="sponsor" whitespace="normalize">
			<_name>Sponsor</_name>
			<_short-desc>The name of a sponsoring organization or institution</_short-desc>
		</element>
		<element type="structural" localName="fileDesc" whitespace="normalize">
			<_name>File Description (fileDesc)</_name>
			<_short-desc>A full bibliographic description of an electronic file</_short-desc>
		</element>
		<element type="structural" localName="titleStmt" whitespace="normalize">
			<_name>Title Statement</_name>
			<_short-desc>Information about the title of a work and those responsible for its intellectual content</_short-desc>
		</element>
		<element type="structural" localName="div" whitespace="normalize">
			<_name>Div</_name>
			<_short-desc>A subdivision of the front, body, or back of a text</_short-desc>
		</element>
		<element type="structural" localName="editorialDecl" whitespace="normalize">
			<_name>Editorial Declaration</_name>
			<_short-desc>Details of editorial principles and practices applied during the encoding of a text</_short-desc>
		</element>
		<element type="span" localName="s" whitespace="normalize">
			<_name>Sentence</_name>
			<_short-desc>A sentence-like division of a text</_short-desc>
		</element>
		<element type="span" localName="gap" whitespace="normalize">
			<_name>Gap</_name>
			<_short-desc>Indicates a point where material has been omitted in a transcription, whether for editorial reasons described in the TEI header, as part of sampling practice, or because the material is illegible or inaudible</_short-desc>
		</element>
		<element type="structural" localName="titlePage" whitespace="normalize">
			<_name>Title Page</_name>
			<_short-desc>The title page of a text, appearing within the front or back matter</_short-desc>
		</element>
		<element type="structural" localName="revisionDesc" whitespace="normalize">
			<_name>Revision Description</_name>
			<_short-desc>Summarizes the revision history for a file</_short-desc>
		</element>
		<element type="span" localName="biblScope" whitespace="normalize">
			<_name>Bibliographic Scope</_name>
			<_short-desc>Defines the scope of a bibliographic reference, for example as a list of pagenumbers, or a named subdivision of a larger work</_short-desc>
		</element>
		<element type="span" localName="speaker" whitespace="normalize">
			<_name>Speaker</_name>
			<_short-desc>A specialized form of heading or label, giving the name of one or more speakers in a dramatic text or fragment</_short-desc>
		</element>
		<element type="span" localName="note" whitespace="normalize">
			<_name>Note</_name>
			<_short-desc>A note of any type</_short-desc>
		</element>
		<element type="structural" localName="biblFull" whitespace="normalize">
			<_name>BiblFull</_name>
			<_short-desc>A fully-structured bibliographic citation, in which all components of the TEI file description are present</_short-desc>
		</element>
		<element type="structural" localName="cit" whitespace="normalize">
			<_name>Citation</_name>
			<_short-desc>A quotation from some other document, together with a bibliographic reference to its source</_short-desc>
		</element>
		<element type="span" localName="ref" whitespace="normalize">
			<_name>Ref</_name>
			<_short-desc>A reference to another location in the current document, in terms of one or more identifiable elements, possibly modified by additional text or comment</_short-desc>
		</element>
		<element type="span" localName="reg" whitespace="normalize">
			<_name>Reg</_name>
			<_short-desc>A reading which has been regularized or normalized in some sense</_short-desc>
		</element>
		<element type="structural" localName="editionStmt" whitespace="normalize">
			<_name>Edition Statement</_name>
			<_short-desc>Groups information relating to one edition of a text</_short-desc>
		</element>
		<element type="structural" localName="langUsage" whitespace="normalize">
			<_name>Language Usage</_name>
			<_short-desc>The languages, sublanguages, registers, dialects etc. represented within a text</_short-desc>
		</element>
		<element type="structural" localName="textClass" whitespace="normalize">
			<_name>Text Classification</_name>
			<_short-desc>Groups information which describes the nature or topic of a text in terms of a standard classification scheme, thesaurus, etc</_short-desc>
		</element>
		<element type="span" localName="p" whitespace="normalize">
			<_name>Paragraph</_name>
			<_short-desc>Marks paragraphs in prose</_short-desc>
		</element>
		<element type="span" localName="sic" whitespace="normalize">
			<_name>Sic</_name>
			<_short-desc>Text reproduced although apparently incorrect or inaccurate</_short-desc>
		</element>
		<element type="span" localName="unclear" whitespace="normalize">
			<_name>Unclear</_name>
			<_short-desc>A word, phrase, or passage which cannot be transcribed with certainty because it is illegible or inaudible in the source</_short-desc>
		</element>
		<element type="span" localName="eg" whitespace="normalize">
			<_name>Example</_name>
			<_short-desc>A single example demonstrating the use of an element or attribute</_short-desc>
		</element>
		<element type="structural" localName="group" whitespace="normalize">
			<_name>Group</_name>
			<_short-desc>The body of a composite text, grouping together a sequence of distinct texts (or groups of such texts) which are regarded as a unit for some purpose, for example the collected works of an author, a sequence of prose essays, etc.</_short-desc>
		</element>
		<element type="structural" localName="back" whitespace="normalize">
			<_name>Back Matter</_name>
			<_short-desc>Contains any appendixes, etc. following the main part of a text</_short-desc>
		</element>
		<element type="structural" localName="refsDecl" whitespace="normalize">
			<_name>References Declaration</_name>
			<_short-desc>Specifies how canonical references are constructed for this text</_short-desc>
		</element>
		<element type="span" localName="time" whitespace="normalize">
			<_name>Time</_name>
			<_short-desc>A phrase defining a time of day in any format</_short-desc>
		</element>
		<element type="span" localName="opener" whitespace="normalize">
			<_name>Opener</_name>
			<_short-desc>Groups together dateline, byline, salutation, and similar phrases appearing as a preliminary group at the start of a division, especially of a letter</_short-desc>
		</element>
		<element type="span" localName="publisher" whitespace="normalize">
			<_name>Publisher</_name>
			<_short-desc>The name of the organization responsible for the publication or distribution of a bibliographic item</_short-desc>
		</element>
		<element type="span" localName="catDesc" whitespace="normalize">
			<_name>Category Description</_name>
			<_short-desc>Describes some category within a taxonomy or text typology, either in the form of a brief prose description or in terms of the situational parameters used by the TEI formal &lt;textDesc&gt;</_short-desc>
		</element>
		<element type="structural" localName="TEI.2" whitespace="normalize">
			<_name>TEI.2</_name>
			<_short-desc>Contains a single TEI-conformant document, comprising a TEI header and a text, either in isolation or as part of a &lt;teiCorpus&gt; element</_short-desc>
		</element>
		<element type="span" localName="creation" whitespace="normalize">
			<_name>Creation</_name>
			<_short-desc>Information about the creation of a text</_short-desc>
		</element>
		<element type="span" localName="soCalled" whitespace="normalize">
			<_name>So Called</_name>
			<_short-desc>A word or phrase for which the author or narrator indicates a disclaiming of responsibility, for example by the use of scare quotes or italics</_short-desc>
		</element>
		<element type="structural" localName="lg" whitespace="normalize">
			<_name>Line Group</_name>
			<_short-desc>A group of verse lines functioning as a formal unit, e.g. a stanza, refrain, verse paragraph, etc.</_short-desc>
		</element>
		<element type="span" localName="docEdition" whitespace="normalize">
			<_name>Document Edition</_name>
			<_short-desc>An edition statement as presented on a title page of a document</_short-desc>
		</element>
		<element type="span" localName="principal" whitespace="normalize">
			<_name>Principal Researcher</_name>
			<_short-desc>The name of the principal researcher responsible for the creation of an electronic text</_short-desc>
		</element>
		<element type="span" localName="lb" whitespace="normalize">
			<_name>Line Break</_name>
			<_short-desc>Marks the start of a new (typographic) line in some edition or version of a text</_short-desc>
		</element>
		<element type="structural" localName="table" whitespace="normalize">
			<_name>Table</_name>
			<_short-desc>Contains text displayed in tabular form, in rows and columns</_short-desc>
		</element>
		<element type="structural" localName="seriesStmt" whitespace="normalize">
			<_name>Series Statement</_name>
			<_short-desc>Groups information about the series, if any, to which a publication belongs</_short-desc>
		</element>
		<element type="span" localName="authority" whitespace="normalize">
			<_name>Authority</_name>
			<_short-desc>The name of a person or other agency responsible for making an electronic file available, other than a publisher or distributor</_short-desc>
		</element>
		<element type="span" localName="docImprint" whitespace="normalize">
			<_name>Document Imprint</_name>
			<_short-desc>The imprint statement (place and date of publication, publisher name), as given (usually) at the foot of a title page</_short-desc>
		</element>
		<element type="span" localName="rs" whitespace="normalize">
			<_name>Referencing String (&lt;rs&gt;)</_name>
			<_short-desc>Contains a general purpose name or referring string</_short-desc>
		</element>
		<element type="structural" localName="body" whitespace="normalize">
			<_name>Body</_name>
			<_short-desc>The whole body of a single unitary text, excluding any front or back matter</_short-desc>
		</element>
		<element type="span" localName="ptr" whitespace="normalize">
			<_name>Pointer</_name>
			<_short-desc>A pointer to another location in the current document in terms of one or more identifiable elements</_short-desc>
		</element>
		<element type="span" localName="docDate" whitespace="normalize">
			<_name>Document Date</_name>
			<_short-desc>The date of a document, as given (usually) on a title page</_short-desc>
		</element>
		<element type="span" localName="salute" whitespace="normalize">
			<_name>Salute</_name>
			<_short-desc>A salutation or greeting prefixed to a foreword, dedicatory epistle, or other division of a text, or the salutation in the closing of a letter, preface, etc.</_short-desc>
		</element>
		<element type="structural" localName="encodingDesc" whitespace="normalize">
			<_name>Encoding Description</_name>
			<_short-desc>Documents the relationship between an electronic text and the source or sources from which it was derived</_short-desc>
		</element>
		<element type="span" localName="resp" whitespace="normalize">
			<_name>Resp</_name>
			<_short-desc>A phrase describing the nature of a person's intellectual responsibility</_short-desc>
		</element>
		<element type="span" localName="distributor" whitespace="normalize">
			<_name>Distributor</_name>
			<_short-desc>The name of a person or other agency responsible for the distribution of a text</_short-desc>
		</element>
		<element type="span" localName="language" whitespace="normalize">
			<_name>Language</_name>
			<_short-desc>Identifies the language being described in the writing system declaration</_short-desc>
		</element>
		<element type="structural" localName="profileDesc" whitespace="normalize">
			<_name>Profile Description</_name>
			<_short-desc>A detailed description of non-bibliographic aspects of a text, specifically the languages and sublanguages used, the situation in which it was produced, the participants and their setting</_short-desc>
		</element>
		<element type="span" localName="classCode" whitespace="normalize">
			<_name>ClassCode</_name>
			<_short-desc>The classification code used for this text in some standard classification system</_short-desc>
		</element>
		<element type="span" localName="hi" whitespace="normalize">
			<_name>Highlighted</_name>
			<_short-desc>Marks a word or phrase as graphically distinct from the surrounding text, for reasons concerning which no claim is made</_short-desc>
		</element>
		<element type="span" localName="funder" whitespace="normalize">
			<_name>Funder</_name>
			<_short-desc>The name of an individual, institution, or organization responsible for the funding of a project or text</_short-desc>
		</element>
		<element type="structural" localName="availability" whitespace="normalize">
			<_name>Availability</_name>
			<_short-desc>Information about the availability of a text, for example any restrictions on its use or distribution, its copyright status, etc.</_short-desc>
		</element>
		<element type="span" localName="item" whitespace="normalize">
			<_name>Item</_name>
			<_short-desc>One component of a list</_short-desc>
		</element>
		<element type="span" localName="pb" whitespace="normalize">
			<_name>Page Break</_name>
			<_short-desc>Marks the boundary between one page of a text and the next in a standard reference system</_short-desc>
		</element>
		<element type="span" localName="byline" whitespace="normalize">
			<_name>Byline</_name>
			<_short-desc>The primary statement of responsibility given for a work on its title page or at the head or end of the work</_short-desc>
		</element>
		<element type="structural" localName="samplingDecl" whitespace="normalize">
			<_name>Sampling Declaration</_name>
			<_short-desc>A prose description of the rationale and methods used in sampling texts in the creation of a corpus or collection</_short-desc>
		</element>
		<element type="span" localName="label" whitespace="normalize">
			<_name>Label</_name>
			<_short-desc>The label associated with an item in a list; in glossaries, marks the term being defined</_short-desc>
		</element>
		<element type="span" localName="catRef" whitespace="normalize">
			<_name>Category Reference</_name>
			<_short-desc>Specifies one or more defined categories within some taxonomy or text typology</_short-desc>
		</element>
		<element type="span" localName="addrLine" whitespace="normalize">
			<_name>Address Line</_name>
			<_short-desc>One line of a postal or other address</_short-desc>
		</element>
		<element type="span" localName="index" whitespace="normalize">
			<_name>Index</_name>
			<_short-desc>Marks a location to be indexed for whatever purpose</_short-desc>
		</element>
		<element type="span" localName="emph" whitespace="normalize">
			<_name>Emphasis</_name>
			<_short-desc>Marks words or phrases which are stressed or emphasized for linguistic or rhetorical effect</_short-desc>
		</element>
		<element type="span" localName="divGen" whitespace="normalize">
			<_name>DivGen</_name>
			<_short-desc>The location at which a textual division generated automatically by a text-processing application is to appear</_short-desc>
		</element>
		<element type="span" localName="interp" whitespace="normalize">
			<_name>Interpretation</_name>
			<_short-desc>An interpretative annotation which can be linked to a span of text</_short-desc>
		</element>
		<element type="span" localName="tagUsage" whitespace="normalize">
			<_name>Tag Usage</_name>
			<_short-desc>Information about the usage of a specific element within a &lt;text&gt;</_short-desc>
		</element>
		<element type="span" localName="dateline" whitespace="normalize">
			<_name>Dateline</_name>
			<_short-desc>A brief description of the place, date, time, etc. of production of a letter, newspaper story, or other work, prefixed or suffixed to it as a kind of heading or trailer</_short-desc>
		</element>
		<element type="span" localName="bibl" whitespace="normalize">
			<_name>Bibliographic Citation (&lt;bibl&gt;)</_name>
			<_short-desc>A loosely-structured bibliographic citation of which the sub-components may or may not be explicitly localNameged</_short-desc>
		</element>
		<element type="structural" localName="argument" whitespace="normalize">
			<_name>Argument</_name>
			<_short-desc>A formal list or prose description of the topics addressed by a subdivision of a text</_short-desc>
		</element>
		<element type="span" localName="extent" whitespace="normalize">
			<_name>Extent</_name>
			<_short-desc>The approximate size of the electronic text as stored on some carrier medium, specified in any convenient units</_short-desc>
		</element>
		<element type="structural" localName="interpGrp" whitespace="normalize">
			<_name>Interpretation Group</_name>
			<_short-desc>Collects together &lt;interp&gt; tags</_short-desc>
		</element>
		<element type="structural" localName="epigraph" whitespace="normalize">
			<_name>Epigraph</_name>
			<_short-desc>A quotation, anonymous or attributed, appearing at the start of a section or chapter, or on a title page</_short-desc>
		</element>
		<element type="span" localName="titlePart" whitespace="normalize">
			<_name>Title Part</_name>
			<_short-desc>A subsection or division of the title of a work, as indicated on a title page</_short-desc>
		</element>
		<element type="span" localName="idno" whitespace="normalize">
			<_name>Id. Number</_name>
			<_short-desc>Any standard or non-standard number used to identify a bibliographic item</_short-desc>
		</element>
		<element type="structural" localName="publicationStmt" whitespace="normalize">
			<_name>Publication Statement</_name>
			<_short-desc>Groups information concerning the publication or distribution of an electronic or other text</_short-desc>
		</element>
		<element type="structural" localName="change" whitespace="normalize">
			<_name>Change</_name>
			<_short-desc>Summarizes a particular change or correction made to a particular version of an electronic text which is shared between several researchers</_short-desc>
		</element>
		<element type="span" localName="gloss" whitespace="normalize">
			<_name>Gloss</_name>
			<_short-desc>A phrase or word used to provide a gloss or definition for some other word or phrase</_short-desc>
		</element>
		<element type="span" localName="mentioned" whitespace="normalize">
			<_name>Mentioned</_name>
			<_short-desc>Marks words or phrases mentioned, not used</_short-desc>
		</element>
		<element type="structural" localName="row" whitespace="normalize">
			<_name>Row</_name>
			<_short-desc>One row of a table</_short-desc>
		</element>
		<element type="span" localName="num" whitespace="normalize">
			<_name>Num</_name>
			<_short-desc>A number, written in any form</_short-desc>
		</element>
		<element type="structural" localName="keywords" whitespace="normalize">
			<_name>Keywords</_name>
			<_short-desc>A list of keywords or phrases identifying the topic or nature of a text</_short-desc>
		</element>
		<element type="span" localName="xref" whitespace="normalize">
			<_name>Extended Reference (&lt;xref&gt;)</_name>
			<_short-desc>A reference to another location in the current document, or an external document, using an extended pointer notation, possibly modified by additional text or comment</_short-desc>
		</element>
		<element type="span" localName="l" whitespace="normalize">
			<_name>Verse Line (&lt;l&gt;)</_name>
			<_short-desc>A single, possibly incomplete, line of verse</_short-desc>
		</element>
		<element type="structural" localName="imprint" whitespace="normalize">
			<_name>Imprint</_name>
			<_short-desc>Groups information relating to the publication or distribution of a bibliographic item</_short-desc>
		</element>
		<element type="structural" localName="address" whitespace="normalize">
			<_name>Address</_name>
			<_short-desc>A postal or other address, for example of a publisher, an organization, or an individual</_short-desc>
		</element>
		<element type="structural" localName="tagsDecl" whitespace="normalize">
			<_name>Tagging Declaration</_name>
			<_short-desc>Provides detailed information about the tagging applied to an SGML or XML document</_short-desc>
		</element>
		<element type="span" localName="ab" whitespace="normalize">
			<_name>Anonymous Block (&lt;ab&gt;)</_name>
			<_short-desc>Contains any arbitrary component-level unit of text, acting as an anonymous container for phrase or inter level elements analogous to, but without the semantic baggage of, a paragraph</_short-desc>
		</element>
		<element type="span" localName="orig" whitespace="normalize">
			<_name>Original Form</_name>
			<_short-desc>The original form of a reading, for which a regularized form is given in an attribute value</_short-desc>
		</element>
		<element type="span" localName="corr" whitespace="normalize">
			<_name>Correct Form</_name>
			<_short-desc>The correct form of a passage apparently erroneous in the copy text</_short-desc>
		</element>
		<element type="span" localName="anchor" whitespace="normalize">
			<_name>Anchor</_name>
			<_short-desc>Attaches an identifier to a point within a text, whether or not it corresponds with a textual element</_short-desc>
		</element>
		<element type="structural" localName="classDecl" whitespace="normalize">
			<_name>Classification Declaration</_name>
			<_short-desc>One or more taxonomies defining any classificatory codes used elsewhere in the text</_short-desc>
		</element>
		<element type="structural" localName="sourceDesc" whitespace="normalize">
			<_name>Source Description</_name>
			<_short-desc>A bibliographic description of the copy text(s) from which an electronic text was derived or generated</_short-desc>
		</element>
		<element type="span" localName="add" whitespace="normalize">
			<_name>Add</_name>
			<_short-desc>Letters, words, or phrases inserted in the text by an author, scribe, annotator, or corrector</_short-desc>
		</element>
		<element type="span" localName="seg" whitespace="normalize">
			<_name>Arbitrary Segment (&lt;seg&gt;)</_name>
			<_short-desc>Any arbitrary phrase-level unit of text (including other &lt;seg&gt; elements)</_short-desc>
		</element>
		<element type="span" localName="name" whitespace="normalize">
			<_name>Name</_name>
			<_short-desc>A proper noun or noun phrase</_short-desc>
		</element>
		<element type="structural" localName="teiHeader" whitespace="normalize">
			<_name>Tei Header</_name>
			<_short-desc>The descriptive and declarative information making up an ‘electronic title page’ prefixed to every TEI-conformant text</_short-desc>
		</element>
		<element type="span" localName="author" whitespace="normalize">
			<_name>Author</_name>
			<_short-desc>The name of the author(s), personal or corporate, of a work; the primary statement of responsibility for any bibliographic item</_short-desc>
		</element>
		<element type="span" localName="pubPlace" whitespace="normalize">
			<_name>Publication Place</_name>
			<_short-desc>The name of the place where a bibliographic item was published</_short-desc>
		</element>
		<element type="span" localName="term" whitespace="normalize">
			<_name>Term</_name>
			<_short-desc>A single-word, multi-word, or symbolic designation which is regarded as a technical term</_short-desc>
		</element>
		<element type="span" localName="date" whitespace="normalize">
			<_name>Date</_name>
			<_short-desc>A date in any format</_short-desc>
		</element>
		<element type="structural" localName="listBibl" whitespace="normalize">
			<_name>ListBibl</_name>
			<_short-desc>A list of bibliographic citations of any kind</_short-desc>
		</element>
		<element type="structural" localName="notesStmt" whitespace="normalize">
			<_name>Notes Statement</_name>
			<_short-desc>Collects together any notes providing information about a text additional to that recorded in other parts of the bibliographic description</_short-desc>
		</element>
		<element type="span" localName="foreign" whitespace="normalize">
			<_name>Foreign</_name>
			<_short-desc>A word or phrase belonging to some language other than that of the surrounding text</_short-desc>
		</element>
		<element type="span" localName="closer" whitespace="normalize">
			<_name>Closer</_name>
			<_short-desc>Groups together dateline, byline, salutation, and similar phrases appearing as a final group at the end of a division, especially of a letter</_short-desc>
		</element>
		<element type="span" localName="rendition" whitespace="normalize">
			<_name>Rendition</_name>
			<_short-desc>Information about the intended rendition of one or more elements</_short-desc>
		</element>
		<element type="span" localName="formula" whitespace="normalize">
			<_name>Formula</_name>
			<_short-desc>A mathematical or other formula</_short-desc>
		</element>
		<element type="structural" localName="front" whitespace="normalize">
			<_name>Front Matter</_name>
			<_short-desc>Any prefatory matter (headers, title page, prefaces, dedications, etc.) found at the start of a document, before the main body</_short-desc>
		</element>
		<element type="span" localName="slocalNamee" whitespace="normalize">
			<_name>Stage Direction (&lt;stage&gt;)</_name>
			<_short-desc>Any kind of stage direction within a dramatic text or fragment</_short-desc>
		</element>
		<element type="structural" localName="div4" whitespace="normalize">
			<_name>Div4</_name>
			<_short-desc>A fourth-level subdivision of the front, body, or back of a text</_short-desc>
		</element>
		<element type="structural" localName="div5" whitespace="normalize">
			<_name>Div5</_name>
			<_short-desc>A fifth-level subdivision of the front, body, or back of a text</_short-desc>
		</element>
		<element type="structural" localName="div2" whitespace="normalize">
			<_name>Div2</_name>
			<_short-desc>A second-level subdivision of the front, body, or back of a text</_short-desc>
		</element>
		<element type="structural" localName="div3" whitespace="normalize">
			<_name>Div3</_name>
			<_short-desc>A third-level subdivision of the front, body, or back of a text</_short-desc>
		</element>
		<element type="span" localName="del" whitespace="normalize">
			<_name>Del</_name>
			<_short-desc>A letter, word or passage deleted, marked as deleted, or otherwise indicated as superfluous or spurious in the copy text by an author, scribe, annotator, or corrector</_short-desc>
		</element>
		<element type="structural" localName="div0" whitespace="normalize">
			<_name>Div0</_name>
			<_short-desc>The largest possible subdivision of the body of a text</_short-desc>
		</element>
		<element type="span" localName="docAuthor" whitespace="normalize">
			<_name>Document Author</_name>
			<_short-desc>The name of the author of the document, as given on the title page (often but not always contained in a &lt;byline&gt;)</_short-desc>
		</element>
		<element type="structural" localName="div1" whitespace="normalize">
			<_name>Div1</_name>
			<_short-desc>A first-level subdivision of the front, body, or back of a text (the largest, if &lt;div0&gt; is not used, the second largest if it is)</_short-desc>
		</element>
		<element type="span" localName="q" whitespace="normalize">
			<_name>Quotation (&lt;q&gt;)</_name>
			<_short-desc>A quotation or apparent quotation — a representation of speech or thought marked as being quoted from someone else (whether in fact quoted or not); in narrative, the words are usually those of of a character or speaker; in dictionaries, &lt;q&gt; may be used to mark real or contrived examples of usage</_short-desc>
		</element>
		<element type="span" localName="abbr" whitespace="normalize">
			<_name>Abbr</_name>
			<_short-desc>An abbreviation of any sort</_short-desc>
		</element>
		<element type="structural" localName="category" whitespace="normalize">
			<_name>Category</_name>
			<_short-desc>An individual descriptive category, possibly nested within a superordinate category, within a user-defined taxonomy</_short-desc>
		</element>
		<element type="span" localName="gi" whitespace="normalize">
			<_name>Generic Identifier (&lt;gi&gt;)</_name>
			<_short-desc>The name (generic identifier) of an element</_short-desc>
		</element>
		<element type="span" localName="milestone" whitespace="normalize">
			<_name>Milestone</_name>
			<_short-desc>Marks the boundary between sections of a text, as indicated by changes in a standard reference system</_short-desc>
		</element>
		<element type="structural" localName="sp" whitespace="normalize">
			<_name>Speech (&lt;sp&gt;)</_name>
			<_short-desc>An individual speech in a performance text, or a passage presented as such in a prose or verse text</_short-desc>
		</element>
		<element type="structural" localName="text" whitespace="normalize">
			<_name>Text</_name>
			<_short-desc>A single text of any kind, whether unitary or composite, for example a poem or drama, a collection of essays, a novel, a dictionary, or a corpus sample</_short-desc>
		</element>
		<element type="structural" localName="taxonomy" whitespace="normalize">
			<_name>Taxonomy</_name>
			<_short-desc>Defines a typology used to classify texts either implicitly, by means of a bibliographic citation, or explicitly by a structured taxonomy</_short-desc>
		</element>
		<element type="structural" localName="div6" whitespace="normalize">
			<_name>Div6</_name>
			<_short-desc>A sixth-level subdivision of the front, body, or back of a text</_short-desc>
		</element>
		<element type="structural" localName="div7" whitespace="normalize">
			<_name>Div7</_name>
			<_short-desc>A seventh-level subdivision of the front, body, or back of a text</_short-desc>
		</element>
		<element type="structural" localName="figure" whitespace="normalize">
			<_name>Figure</_name>
			<_short-desc>Indicates the location of a graphic, illustration, or figure</_short-desc>
		</element>
		<element type="span" localName="editor" whitespace="normalize">
			<_name>Editor</_name>
			<_short-desc>Secondary statement of responsibility for a bibliographic item, for example the name of an individual, institution or organization, (or of several such) acting as editor, compiler, translator, etc.</_short-desc>
		</element>
		<element type="span" localName="trailer" whitespace="normalize">
			<_name>Trailer</_name>
			<_short-desc>A closing title or footer appearing at the end of a division of a text</_short-desc>
		</element>
		<element type="structural" localName="list" whitespace="normalize">
			<_name>List</_name>
			<_short-desc>Any sequence of items organized as a list</_short-desc>
		</element>
		<element type="span" localName="head" whitespace="normalize">
			<_name>Head</_name>
			<_short-desc>Any heading, for example, the title of a section, or the heading of a list or glossary</_short-desc>
		</element>
		<element type="span" localName="title" whitespace="normalize">
			<_name>Title</_name>
			<_short-desc>The title of a work, whether article, book, journal, or series, including any alternative titles or subtitles</_short-desc>
		</element>
		<element type="structural" localName="projectDesc" whitespace="normalize">
			<_name>Project Description</_name>
			<_short-desc>Describes in detail the aim or purpose for which an electronic file was encoded, together with any other relevant information concerning the process by which it was assembled or collected</_short-desc>
		</element>
		<element type="span" localName="figDesc" whitespace="normalize">
			<_name>Figure Description</_name>
			<_short-desc>A brief prose description of the appearance or content of a graphic figure, for use when documenting an image without displaying it</_short-desc>
		</element>

		<!-- TEI Lite specific -->
		
		<element type="span" localName="code" whitespace="normalize">
			<_name>Code</_name>
			<_short-desc>A short fragment of code in some formal language (often a programming language)</_short-desc>
		</element>
		<element type="span" localName="ident" whitespace="normalize">
			<_name>Ident</_name>
			<_short-desc>An identifier of some kind, e.g. a variable name or the name of an SGML element or attribute</_short-desc>
		</element>
		<element type="span" localName="kw" whitespace="normalize">
			<_name>Kw</_name>
			<_short-desc>A keyword in some formal language</_short-desc>
		</element>
	</element-list>
</dispspec>

<?xml version="1.0"?>
<!-- Experimental XML-based DispSpec for DocBook -->
<dispspec>

	<metadata>
		<_name>DocBook</_name>
		<_description>DocBook is a document type that broadly corresponds to what most people think of as "a book"; it is well-suited for writing books and articles, for both print and web-based publication.</_description>
	</metadata>
	
	<!-- Specify the DTD that this annotates; hence we can get at the DTD if it wasn't supplied in the document: -->
	<document-models>
		<external-document-model type="dtd" public-id="-//OASIS//DTD DocBook XML V4.1.2//EN" system-id="http://www.oasis-open.org/docbook/xml/4.1.2/docbookx.dtd"/>
		<!-- this allows room for other types of document model e.g. W3C XML Schema, RELAX NG; and we can even embed schema in-place below here... -->
	</document-models>

	<element-list>
			
		<!-- The high-level divisions -->
	
		<element tag="set" type="structural" color="a0b0c0" icon="cong-docbook-set">
			<_name>Set</_name>
			<_short-desc>A collection of books.</_short-desc>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="book" type="structural" color="a0b0c0" icon="cong-docbook-book">
			<_name>Book</_name>
			<_short-desc>A book, with a title, and perhaps with some bibliographical material, in addition to the actual content of the book.</_short-desc>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="bookinfo" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Book Information</_name>
			<_short-desc>Meta-information for a book</_short-desc>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="article" type="structural" color="a0b0c0" icon="cong-docbook-article">
			<_name>Article</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="artheader" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Article Header</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="title" type="structural" color="a0b0c0" icon="cong-title">
			<_name>Title</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="subtitle" type="structural" color="a0b0c0" icon="cong-title">
			<_name>Subtitle</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="authorgroup" type="structural" color="a0b0c0" icon="cong-wrapper">
			<_name>Authorship Information</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="author" type="structural" color="a0b0c0" icon="cong-author"> 
			<_name>Author</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="editor" type="structural" color="a0b0c0" icon="cong-editor">
			<_name>Editor</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="publisher" type="structural" color="a0b0c0" icon="cong-publisher">
			<_name>Publisher</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="publishername" type="structural" color="a0b0c0" icon="cong-name">
			<_name>Publisher Name</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="address" type="structural" color="a0b0c0" icon="cong-address">
			<_name>Address</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="printhistory" type="structural" color="a0b0c0" icon="cong-history">
			<_name>Printing History</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="isbn" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>ISBN</_name>
			<_short-desc>The International Standard Book Number of a document.</_short-desc>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="issn" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>ISSN</_name>
			<_short-desc>The International Standard Serial Number of a periodical.</_short-desc>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="releaseinfo" type="structural" color="a0b0c0" icon="cong-history">
			<_name>Release Information</_name>
			<_short-desc>A brief description of the version of a document.</_short-desc>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="pubdate" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Date of Publication</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="copyright" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Copyright</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="year" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Year</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="holder" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Copyright Holder</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="docinfo" type="structural" color="a0b0c0"  icon="cong-metadata">
			<_name>Bibliographic Information</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="abstract" type="structural" color="a0b0c0"  icon="cong-metadata">
			<_name>Abstract</_name>
			<_short-desc>A summary.</_short-desc>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="para" type = "plugin" plugin-id="paragraph" icon="cong-paragraph">
			<_name>Paragraph</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="formalpara" type = "structural" icon="cong-paragraph">
			<_name>Paragraph (titled)</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="legalnotice" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Legal Notice</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="colophon" type="structural" color="a0b0c0" icon="cong-metadata"> 
			<_name>Colophon</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="qandaset" type="structural" color="a0b0c0" icon="cong-faq">
			<_name>Question-and-Answer Set</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="qandadiv" type="structural" color="a0b0c0" icon="cong-wrapper">
			<_name>Question-and-Answer Subset</_name>

			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>


		<element tag="informalexample" type="structural" color="a0b0c0">
			<_name>Example (informal)</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="informaltable" type="structural" color="a0b0c0" icon="cong-table">
			<_name>Table (informal)</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="simplelist" type="structural" color="a0b0c0" icon="cong-list">
			<_name>List (simple)</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<!-- List support -->
		<element tag="itemizedlist" type="structural" color="a0b0c0" icon="cong-bulleted-list">

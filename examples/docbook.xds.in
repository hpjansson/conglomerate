			<_name>Image Object</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="imagedata" type="embed-external-file" filename-xpath="@fileref" color="a0b0c0">
			<_name>Image Data</_name>
			<!-- fileref is assumed for now -->
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="textobject" type="structural" color="a0b0c0">
			<_name>Text Object</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="caption" type="structural" color="a0b0c0">
			<_name>Caption</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="graphic" type="embed-external-file" filename-xpath="@fileref" color="a0b0c0">
			<_name>Graphic</_name>	
			<!-- fileref is assumed for now -->
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="programlisting" type="structural" color="a0b0c0" icon="cong-code-fragment" whitespace="preserve">
			<!-- 
				perhaps this should use a fixed-space font
				Perhaps we need a boolean to determine whether something
				should appear in the regular proportional spacing or in
				fixed-space? (defaults to proportional).
				Could also do with a verbatim flag which respects carriage returns...

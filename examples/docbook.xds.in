			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<!-- The various sections -->
		<element tag="preface" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Preface</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="part" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Part</_name>
			<_short-desc>A high-level division of a book.</_short-desc>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="partintro" type="structural" color="a0b0c0">
			<_name>Introduction</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="chapter" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Chapter</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="sect1" type="structural" color="a1b0c0" icon="cong-subsection">

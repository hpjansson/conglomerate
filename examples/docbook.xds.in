			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="variablelist" type="structural" color="a0b0c0" icon="cong-definition-list">
			<_name>List (terms and definitions)</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="varlistentry" type="structural" color="a0b0c0" icon="cong-wrapper">

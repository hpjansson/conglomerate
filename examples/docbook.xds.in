		</element>

		<element tag="qandaentry" type="structural" color="a0b0c0" icon="cong-faq">
			<_name>Question-and-Answer</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="question" type="structural" color="a0b0c0" icon="cong-question">
			<_name>Question</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="answer" type="structural" color="a0b0c0" icon="cong-answer">

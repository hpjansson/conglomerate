		<element tag="appendix" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Appendix</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="glossary" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Glossary</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="glossentry" type="structural" color="a0b0c0" icon="cong-entry">
			<_name>Glossary Entry</_name>
			<header-info tag="glossterm"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="glossterm" type="structural" color="a0b0c0">
			<_name>Term</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>

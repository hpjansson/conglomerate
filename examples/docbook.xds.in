		<element tag="screenshot" type="structural" color="a0b0c0">
			<_name>Screenshot</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="informalfigure" type="structural" color="a0b0c0">
			<_name>Figure (informal)</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="mediaobject" type="structural" color="a0b0c0">
			<_name>Media Object</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>

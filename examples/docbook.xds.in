		<element tag="entrytbl" type="structural" color="a0b0c0">
			<_name>Table Entry (subtable)</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<!-- Miscellanous stuff -->
		<element tag="anchor" type="structural" color="a0b0c0">
			<!-- FIXME:  EMPTY content model should have a special type of tag -->
			<_name>Anchor Point</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="figure" type="structural" color="a0b0c0">

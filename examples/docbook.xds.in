			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<!-- Admonitions -->
		<element tag="caution" type="plugin" plugin-id="admonition" color="a0b0c0">
			<_name>Caution</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
			<key-value-list>
				<key-value-pair key="icon" value="cong-caution-48.png" />
			</key-value-list>
		</element>
		<element tag="important" type="plugin" plugin-id="admonition" color="a0b0c0">
			<_name>Important</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
			<key-value-list>
				<key-value-pair key="icon" value="cong-important-48.png" />
			</key-value-list>
		</element>
		<element tag="note" type="plugin" plugin-id="admonition" color="a0b0c0">
			<_name>Note</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
			<key-value-list>
				<key-value-pair key="icon" value="cong-note-48.png" />
			</key-value-list>
		</element>
		<element tag="tip" type="plugin" plugin-id="admonition" color="a0b0c0">
			<_name>Tip</_name>

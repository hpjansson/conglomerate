			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="sect3" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Subsection (level 3)</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="sect4" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Subsection (level 4)</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="sect5" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Section (level 5)</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="reference" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Reference</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refentry" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Reference Page</_name>
			<header-info xpath="normalize-space(child::refmeta/refentrytitle)"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refmeta" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Metadata</_name>
			<_short-desc>Meta-information about a Reference Page</_short-desc>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refentrytitle" type="structural" color="a0b0c0" icon="cong-title">
			<_name>Title</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refmiscinfo" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Additional metadata</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="indexterm" type="structural" color="a0b0c0" icon="cong-indexterm">
			<_name>Index Term</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>


		<element tag="refnamediv" type="structural" color="a0b0c0" icon="cong-metadata">
			<_name>Naming</_name>
			<_short-desc>Name, purpose and classification about a Reference Page</_short-desc>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refname" type="structural" color="a0b0c0" icon="cong-title">
			<_name>Name</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refpurpose" type="structural" color="a0b0c0" icon="cong-summary">
			<_name>Purpose</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refsynopsisdiv" type="structural" color="a0b0c0" icon="cong-wrapper">
			<_name>Synopsis (syntax)</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refsect1" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Section</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refsect2" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Subsection</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="refsect3" type="structural" color="a0b0c0" icon="cong-subsection">
			<_name>Subsubsection</_name>
			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>

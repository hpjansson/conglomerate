			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="bibliography" type="structural" color="a0b0c0" icon="cong-bibliography">
			<_name>Bibliography</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="biblioentry" type="structural" color="a0b0c0" icon="cong-entry">

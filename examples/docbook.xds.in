			<key-value-list>
				<key-value-pair key="icon" value="cong-tip-48.png" />
			</key-value-list>
		</element>
		<element tag="warning" type="plugin" plugin-id="admonition" color="a0b0c0">
			<_name>Warning</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
			<key-value-list>
				<key-value-pair key="icon" value="cong-warning-48.png" />
			</key-value-list>
		</element>

		<!-- Tables -->
		<!-- FIXME: this needs a throrough overhaul: -->
		<element tag="tgroup" type="structural" color="a0b0c0">
			<_name>Table Group</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="thead" type="structural" color="a0b0c0">
			<_name>Table Header</_name>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="tfoot" type="structural" color="a0b0c0">

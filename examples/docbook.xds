<?xml version="1.0"?>
<!-- Experimental XML-based DispSpec for DocBook -->
<dispspec>

	<metadata>
		<name xml:lang="en">DocBook</name>
		<description xml:lang="en">DocBook is a document type that broadly corresponds to what most people think of as "a book"; it is well-suited for writing books and articles, for both print and web-based publication.</description>
	</metadata>
	
	<!-- Specify the DTD that this annotates; hence we can get at the DTD if it wasn't supplied in the document: -->
	<document-models>
		<external-document-model type="dtd" public-id="-//OASIS//DTD DocBook XML V4.1.2//EN" system-id="http://www.oasis-open.org/docbook/xml/4.1.2/docbookx.dtd"/>
		<!-- this allows room for other types of document model e.g. W3C XML Schema, RELAX NG; and we can even embed schema in-place below here... -->
	</document-models>

	<element-list>
			
		<!-- The high-level divisions -->
	
		<element tag="set" type="structural" color="a0b0c0" icon="cong-docbook-set">
			<name xml:lang="en">Set</name>
			<short-desc xml:lang="en">A collection of books.</short-desc>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>

		<element tag="book" type="structural" color="a0b0c0" icon="cong-docbook-book">
			<name xml:lang="en">Book</name>

			<!-- Experimental translations by Dave Malcolm; please replace if you're a native speaker of these xml:langs -->
			<name xml:lang="fr">Livre</name>
			<name xml:lang="jp">&#x672C;</name>

			<short-desc xml:lang="en">A book, with a title, and perhaps with some bibliographical material, in addition to the actual content of the book.</short-desc>
			<short-desc xml:lang="fr">FIXME: THIS IS A TEST STRING.</short-desc>

			<header-info tag="title"></header-info>
			<property-dialog plugin-id="docbook-generic-node-properties"/>
		</element>
		<element tag="bookinfo" type="structural" color="a0b0c0" icon="cong-metadata">
			<name xml:lang="en">Book Information</name>

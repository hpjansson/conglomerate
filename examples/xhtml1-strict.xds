<?xml version="1.0"?>
<dispspec>
	<metadata>
		<name xml:lang="en">XHTML 1.0 (strict)</name>
		<description xml:lang="en">The strict XML version of HTML </description>
	</metadata>
	<serialisation>
		<format extension="html"/>
	</serialisation>
	<document-models>
		<external-document-model type="dtd" public-id="-//W3C//DTD XHTML 1.0 Strict//EN" system-id="http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"/>
	</document-models>
	<element-list>
	
		<!-- Skeletal Tags -->
		<element tag="body" type="structural">
			<name xml:lang="en">Body</name>
		</element>
		<element tag="head" type="structural">
			<name xml:lang="en">Head</name>
		</element>
		<element tag="html" type="structural">
			<name xml:lang="en">HTML Page</name>
		</element>

		<!-- Header Tags -->
		<element tag="base" type="structural">
			<name xml:lang="en">Base</name>
		</element>
		<element tag="link" type="structural">
			<name xml:lang="en">Link</name>
		</element>
		<element tag="meta" type="structural">
			<name xml:lang="en">Meta</name>
		</element>
		<element tag="style" type="structural">
			<name xml:lang="en">Style</name>
		</element>
		<element tag="title" type="structural">
			<name xml:lang="en">Title</name>
		</element>

		<!-- Hyperlinks -->
		<element tag="a" type="span">
			<name xml:lang="en">Anchor</name>
		</element>

		<!-- Text Markup: Physical-based -->
		<element tag="b" type="span">
			<name xml:lang="en">Bold</name>
		</element>
		<element tag="big" type="span">
			<name xml:lang="en">Big</name>
		</element>
		<element tag="i" type="span">
			<name xml:lang="en">Italic</name>
		</element>
		<element tag="small" type="span">
			<name xml:lang="en">Small</name>
		</element>
		<element tag="sub" type="span">
			<name xml:lang="en">Subscript</name>
		</element>
		<element tag="tt" type="span">
			<name xml:lang="en">Teletype</name>
		</element>

		<!-- Text Markup: Content-based -->
		<element tag="cite" type="span">
			<name xml:lang="en">Citation</name>
		</element>
		<element tag="code" type="span">
			<name xml:lang="en">Code</name>
		</element>
		<element tag="dfn" type="span">
			<name xml:lang="en">Definition</name>
		</element>
		<element tag="em" type="span">
			<name xml:lang="en">Emphasis</name>
		</element>
		<element tag="kbd" type="span">
			<name xml:lang="en">Keyboard</name>
		</element>
		<element tag="samp" type="span">
			<name xml:lang="en">Samp</name>
		</element>
		<element tag="strong" type="span">
			<name xml:lang="en">Strong</name>
		</element>
		<element tag="var" type="span">
			<name xml:lang="en">Var</name>
		</element>

		<!-- Lists -->
		<element tag="dl" type="structural">
			<name xml:lang="en">Dl</name>
		</element>
		<element tag="li" type="plugin" plugin-id="listitem">
			<name xml:lang="en">List Item</name>
		</element>
		<element tag="ol" type="structural">
			<name xml:lang="en">List (ordered)</name>
		</element>
		<element tag="ul" type="structural">
			<name xml:lang="en">List (unordered)</name>
		</element>

		<!-- Forms -->
		<element tag="form" type="structural">
			<name xml:lang="en">Form</name>
		</element>
		<element tag="input" type="structural">
			<name xml:lang="en">Input</name>
		</element>
		<element tag="option" type="structural">
			<name xml:lang="en">Option</name>
		</element>
		<element tag="select" type="structural">
			<name xml:lang="en">Select</name>
		</element>
		<element tag="textarea" type="structural">
			<name xml:lang="en">Textarea</name>
		</element>

		<!-- Rules, images and multimedia -->
		<element tag="img" type="structural">
			<name xml:lang="en">Img</name>
		</element>
		<element tag="map" type="structural">
			<name xml:lang="en">Map</name>
		</element>

		<!-- Tables -->
		<element tag="caption" type="span">
			<name xml:lang="en">Caption</name>
		</element>
		<element tag="col" type="span">
			<name xml:lang="en">Col</name>
		</element>
		<element tag="colgroup" type="structural">
			<name xml:lang="en">Colgroup</name>
		</element>
		<element tag="table" type="structural">
			<name xml:lang="en">Table</name>
		</element>
		<element tag="tbody" type="structural">
			<name xml:lang="en">Tbody</name>
		</element>
		<element tag="td" type="span">
			<name xml:lang="en">Td</name>
		</element>
		<element tag="tfoot" type="structural">
			<name xml:lang="en">Tfoot</name>
		</element>
		<element tag="th" type="span">
			<name xml:lang="en">Th</name>
		</element>
		<element tag="thead" type="structural">
			<name xml:lang="en">Thead</name>
		</element>

		<!-- Executable Content -->
		<element tag="object" type="structural">
			<name xml:lang="en">Object</name>
		</element>
		<element tag="script" type="structural">
			<name xml:lang="en">Script</name>
		</element>

		<!-- Content Presentation and Flow -->
		<element tag="address" type="span">
			<name xml:lang="en">Address</name>
		</element>
		<element tag="blockquote" type="structural">
			<name xml:lang="en">Blockquote</name>
		</element>
		<element tag="br" type="span">
			<name xml:lang="en">Br</name>
		</element>
		<element tag="div" type="span">
			<name xml:lang="en">Div</name>
		</element>
		<element tag="h1" type="structural">
			<name xml:lang="en">H1</name>
		</element>
		<element tag="h2" type="structural">
			<name xml:lang="en">H2</name>
		</element>
		<element tag="h3" type="structural">
			<name xml:lang="en">H3</name>
		</element>
		<element tag="h4" type="structural">
			<name xml:lang="en">H4</name>
		</element>
		<element tag="h5" type="structural">
			<name xml:lang="en">H5</name>
		</element>
		<element tag="h6" type="structural">
			<name xml:lang="en">H6</name>
		</element>
		<element tag="hr" type="structural">
			<name xml:lang="en">Horizontal Rule</name>
		</element>
		<element tag="p" type="structural">
			<name xml:lang="en">Paragraph</name>
		</element>

		<!-- Unclassified tags -->

		<element tag="param" type="span">
			<name xml:lang="en">Param</name>
		</element>

		<element tag="button" type="span">
			<name xml:lang="en">Button</name>
		</element>
		<element tag="dt" type="span">
			<name xml:lang="en">Dt</name>
		</element>

		<!-- Ongoing classification -->
		<element tag="area" type="span">
			<name xml:lang="en">Area</name>
		</element>
		<element tag="dd" type="span">
			<name xml:lang="en">Dd</name>
		</element>
		<element tag="fieldset" type="span">
			<name xml:lang="en">Fieldset</name>
		</element>
		<element tag="ins" type="span">
			<name xml:lang="en">Ins</name>
		</element>
		<element tag="span" type="span">
			<name xml:lang="en">Span</name>
		</element>
		<element tag="label" type="span">
			<name xml:lang="en">Label</name>
		</element>
		<element tag="acronym" type="span">
			<name xml:lang="en">Acronym</name>
		</element>
		<element tag="noscript" type="structural">
			<name xml:lang="en">Noscript</name>
		</element>
		<element tag="sup" type="span">
			<name xml:lang="en">Superscript</name>
		</element>
		<element tag="optgroup" type="structural">
			<name xml:lang="en">Optgroup</name>
		</element>
		<element tag="pre" type="span">
			<name xml:lang="en">Preformatted Text</name>
		</element>
		<element tag="bdo" type="span">
			<name xml:lang="en">Bdo</name>
		</element>
		<element tag="tr" type="structural">
			<name xml:lang="en">Tr</name>
		</element>
		<element tag="del" type="span">
			<name xml:lang="en">Del</name>
		</element>
		<element tag="q" type="span">
			<name xml:lang="en">Q</name>
		</element>
		<element tag="abbr" type="span">
			<name xml:lang="en">Abbr</name>
		</element>
		<element tag="legend" type="span">
			<name xml:lang="en">Legend</name>
		</element>
	</element-list>
</dispspec>

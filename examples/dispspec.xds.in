<?xml version="1.0"?>
<dispspec>
  <metadata>
    <_name>Conglomerate Display Specification</_name>
    <_description>Information that tells Conglomerate how to display a particular type of file.</_description>
  </metadata>
  <serialisation>
    <format extension="xds"/>
  </serialisation>
  <document-models>
	<external-document-model type="dtd" public-id="-//Conglomerate//Conglomerate Display Specification V0.1.0//EN"/>
  </document-models>
  <element-list>
    <element localName="collapseto" type="structural">
      <_name>Collapseto</_name>
    </element>
    <element localName="description" type="structural">
      <_name>Description</_name>
      <_short-desc>Short Description of the document type</_short-desc>
    </element>
    <element localName="dispspec" type="structural">
      <_name>Conglomerate Display Specification</_name>
    </element><element localName="document-models" type="structural">
      <_name>Document Models</_name>
    </element><element localName="external-document-model" type="structural">
      <_name>External Document Model</_name>
    </element>
    <element localName="element" type="structural">
      <_name>XML Element</_name>
      <header-info xpath="concat(&quot;&lt;&quot;, @localName, &quot;&gt;&quot;)"/>
    </element>
    <element localName="element-list" type="structural">
      <_name>List of XML Elements</_name>
    </element>
    <element localName="format" type="structural">
      <_name>Format</_name>
      <header-info xpath="concat(&quot;filename extension &quot;, @extension)"/>
    </element>
    <element localName="gui" type="structural">
      <_name>GUI</_name>
    </element>
    <element localName="header-info" type="structural">
      <_name>Header Information</_name>
    </element>
    <element localName="metadata" type="structural">
      <_name>Metadata</_name>
    </element>
    <element localName="name" type="structural">
      <_name>Name</_name>
      <_short-desc>Name of an XML element</_short-desc>
    </element>
    <element localName="serialisation" type="structural">
      <_name>Serialisation Type List</_name>
      <_short-desc>List of file types for this type of document</_short-desc>
    </element>
    <element localName="short-desc" type="structural">
      <_name>Short Description</_name>
      <_short-desc>Short Description of an XML element</_short-desc>
    </element>
  </element-list>

</dispspec>

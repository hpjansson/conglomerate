<?xml version="1.0"?>
<!-- Experimental DispSpec for Webpages using Norman Walsh's "website" format  -->
<dispspec>

	<metadata>
		<name>Webpage</name>
		<description></description>
	</metadata>
	
	<!-- probably should specify the DTD that this annotates; hence we can get at the DTD if none was supplied in the document -->
	<document-models/>

	<element-list>
			
		<!-- The high-level divisions -->
		<element tag="webpage" type="structural" color="a0b0c0">
			<name locale="en">Web Page</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="title" type="structural" color="a0b0c0">
			<name locale="en">Title</name>
		</element>
		<element tag="subtitle" type="structural" color="a0b0c0">
			<name locale="en">Subtitle</name>
		</element>
		<element tag="authorgroup" type="structural" color="a0b0c0">
			<name locale="en">Authorship Information</name>
		</element>
		<element tag="author" type="structural" color="a0b0c0">
			<name locale="en">Author</name>
		</element>
		<element tag="editor" type="structural" color="a0b0c0">
			<name locale="en">Editor</name>
		</element>
		<element tag="publisher" type="structural" color="a0b0c0">
			<name locale="en">Publisher</name>
		</element>
		<element tag="publishername" type="structural" color="a0b0c0">
			<name locale="en">Publisher Name</name>
		</element>
		<element tag="address" type="structural" color="a0b0c0">
			<name locale="en">Address</name>
		</element>
		<element tag="printhistory" type="structural" color="a0b0c0">
			<name locale="en">Printing History</name>
		</element>
		<element tag="isbn" type="structural" color="a0b0c0">
			<name locale="en">ISBN</name>
			<short-desc>The International Standard Book Number of a document.</short-desc>
		</element>
		<element tag="issn" type="structural" color="a0b0c0">
			<name locale="en">ISSN</name>
			<short-desc>The International Standard Serial Number of a periodical.</short-desc>
		</element>
		<element tag="releaseinfo" type="structural" color="a0b0c0">
			<name locale="en">Release Information</name>
			<short-desc>A brief description of the version of a document.</short-desc>
		</element>
		<element tag="pubdate" type="structural" color="a0b0c0">
			<name locale="en">Date of Publication</name>
		</element>
		<element tag="copyright" type="structural" color="a0b0c0">
			<name locale="en">Copyright</name>
		</element>
		<element tag="year" type="structural" color="a0b0c0">
			<name locale="en">Year</name>
		</element>
		<element tag="holder" type="structural" color="a0b0c0">
			<name locale="en">Copyright Holder</name>
		</element>
		<element tag="docinfo" type="structural" color="a0b0c0">
			<name locale="en">Bibliographic Information</name>
		</element>
		<element tag="abstract" type="structural" color="a0b0c0">
			<name locale="en">Abstract</name>
			<short-desc>A sumamry.</short-desc>
		</element>
		<element tag="para" type = "structural">
			<name locale="en">Paragraph</name>
		</element>
		<element tag="legalnotice" type="structural" color="a0b0c0">
			<name locale="en">Legal Notice</name>
		</element>
		<element tag="colophon" type="structural" color="a0b0c0">
			<name locale="en">Colophon</name>
		</element>

		<element tag="qandaset" type="structural" color="a0b0c0">
			<header-info tag="title"></header-info>
		</element>

		<element tag="qandadiv" type="structural" color="a0b0c0">
			<header-info tag="title"></header-info>
		</element>

		<element tag="qandaentry" type="structural" color="a0b0c0">
		</element>

		<element tag="question" type="structural" color="a0b0c0">
		</element>

		<element tag="answer" type="structural" color="a0b0c0">
		</element>

		<element tag="bibliography" type="structural" color="a0b0c0">
			<name locale="en">Bibliography</name>
		</element>

		<element tag="biblioentry" type="structural" color="a0b0c0">
			<name locale="en">Entry</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="bibliodiv" type="structural" color="a0b0c0">
			<name locale="en">Section</name>
		</element>

		<element tag="bridgehead" type="structural" color="a0b0c0">
			<name locale="en">Free-floating Heading</name>
		</element>

		<!-- The various sections -->
		<element tag="preface" type="structural" color="a0b0c0">
			<name locale="en">Preface</name>
			<header-info tag="title"></header-info>
		</element>
		<element tag="part" type="structural" color="a0b0c0">
			<name locale="en">Part</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="partintro" type="structural" color="a0b0c0">
			<name locale="en">Part Introduction</name>
		</element>

		<element tag="chapter" type="structural" color="a0b0c0">
			<name locale="en">Chapter</name>
			<header-info tag="title"></header-info>
		</element>
		<element tag="sect1" type="structural" color="a1b0c0">
			<name locale="en">Section</name>
			<short-desc>A top-level section of document</short-desc>
			<header-info tag="title"></header-info>
		</element>
		<element tag="sect2" type="structural" color="a0b0c0">
			<name locale="en">Subsection</name>
			<short-desc>A subsection within a section</short-desc>
			<header-info tag="title"></header-info>
		</element>
		<element tag="sect3" type="structural" color="a0b0c0">
			<name locale="en">Subsection (level 3)</name>
			<header-info tag="title"></header-info>
		</element>
		<element tag="sect4" type="structural" color="a0b0c0">
			<name locale="en">Subsection (level 4)</name>
			<header-info tag="title"></header-info>
		</element>
		<element tag="sect5" type="structural" color="a0b0c0">
			<name locale="en">Section (level 5)</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="reference" type="structural" color="a0b0c0">
			<name locale="en">Reference</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="refentry" type="structural" color="a0b0c0">
			<name locale="en">Reference Page</name>
			<header-info tag="refname"></header-info>
		</element>

		<element tag="refmeta" type="structural" color="a0b0c0">
			<name locale="en">Metadata</name>
			<short-desc locale="en">Meta-information about a Reference Page</short-desc>
		</element>

		<element tag="refentrytitle" type="structural" color="a0b0c0">
			<name locale="en">Title</name>
		</element>

		<element tag="refmiscinfo" type="structural" color="a0b0c0">
			<name locale="en">Additional metadata</name>
		</element>

		<element tag="indexterm" type="structural" color="a0b0c0">
			<name locale="en">Index Term</name>
		</element>


		<element tag="refnamediv" type="structural" color="a0b0c0">
			<name locale="en">Naming</name>
			<short-desc locale="en">Name, purpose and classification about a Reference Page</short-desc>
		</element>

		<element tag="refname" type="structural" color="a0b0c0">
			<name locale="en">Name</name>
		</element>

		<element tag="refpurpose" type="structural" color="a0b0c0">
			<name locale="en">Purpose</name>
		</element>

		<element tag="refsynopsisdiv" type="structural" color="a0b0c0">
			<name locale="en">Synopsis (syntax)</name>
		</element>

		<element tag="refsect1" type="structural" color="a0b0c0">
			<name locale="en">Section</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="refsect2" type="structural" color="a0b0c0">
			<name locale="en">Subsection</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="refsect3" type="structural" color="a0b0c0">
			<name locale="en">Subsubsection</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="appendix" type="structural" color="a0b0c0">
			<name locale="en">Appendix</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="glossary" type="structural" color="a0b0c0">
			<name locale="en">Glossary</name>
		</element>

		<element tag="glossentry" type="structural" color="a0b0c0">
			<name locale="en">Glossary Entry</name>
			<header-info tag="glossterm"></header-info>
		</element>

		<element tag="glossterm" type="structural" color="a0b0c0">
			<name locale="en">Term</name>
		</element>

		<element tag="glossdef" type="structural" color="a0b0c0">
			<name locale="en">Definition</name>
		</element>


		<element tag="informalexample" type="structural" color="a0b0c0">
			<name locale="en">Example (informal)</name>
		</element>

		<element tag="informaltable" type="structural" color="a0b0c0">
			<name locale="en">Table (informal)</name>
		</element>

		<element tag="simplelist" type="structural" color="a0b0c0">
			<name locale="en">List (simple)</name>
		</element>

		<!-- List support -->
		<element tag="itemizedlist" type="structural" color="a0b0c0">
			<name locale="en">List (itemised)</name>
		</element>
		<element tag="orderedlist" type="structural" color="a0b0c0">
			<name locale="en">List (ordered)</name>
		</element>

		<element tag="listitem" type="structural" color="a0b0c0">
		</element>

		<element tag="member" type="structural" color="a0b0c0">
		</element>

		<element tag="variablelist" type="structural" color="a0b0c0">
		</element>

		<element tag="varlistentry" type="structural" color="a0b0c0">
		</element>

		<!-- Miscellanous stuff -->

		<element tag="figure" type="structural" color="a0b0c0">
		</element>

		<element tag="screenshot" type="structural" color="a0b0c0">
		</element>

		<element tag="informalfigure" type="structural" color="a0b0c0">
		</element>

		<element tag="mediaobject" type="structural" color="a0b0c0">
		</element>

		<element tag="imageobject" type="structural" color="a0b0c0">
		</element>

		<element tag="imagedata" type="embed-external-file" filename-attribute="fileref" color="a0b0c0">
			<!-- fileref is assumed for now -->
		</element>

		<element tag="textobject" type="structural" color="a0b0c0">
		</element>

		<element tag="anchor" type="structural" color="a0b0c0">
			<!-- really it's just a placemarker; need a new tag type; posisbly should have an icon for this -->
		</element>

		<element tag="caption" type="structural" color="a0b0c0">
		</element>

		<element tag="graphic" type="embed-external-file" filename-attribute="fileref" color="a0b0c0">
			<!-- fileref is assumed for now -->
		</element>

		<element tag="programlisting" type="structural" color="a0b0c0">
			<!-- 
				perhaps this should use a fixed-space font
				Perhaps we need a boolean to determine whether something
				should appear in the regular proportional spacing or in
				fixed-space? (defaults to proportional).
				Could also do with a verbatim flag which respects carriage returns...
				Even better would be to invoke a Scintilla control and have syntax highlighting!
			-->
			
		</element>

		<element tag="funcsynopsis" type="structural" color="a0b0c0">
			<!-- various related tags need to be added -->
		</element>

		<element tag="screen" type="structural" color="a0b0c0">
			<name locale="en">Screen</name>
		</element>

		<element tag="cmdsynopsis" type="structural" color="a0b0c0">
			<name locale="en">Command Synopsis</name>
		</element>

		<element tag="procedure" type="structural" color="a0b0c0">
			<name locale="en">Procedure</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="step" type="structural" color="a0b0c0">
			<name locale="en">Step</name>
			<header-info tag="title"></header-info>
		</element>

		<element tag="substeps" type="structural" color="a0b0c0">
			<name locale="en">Substeps</name>
		</element>


		<!-- Various inline elements, done as spans follow: -->

		<element tag="acronym" type="span" color="a0b0c0">
			<name locale="en">Acronym</name>
		</element>
 		<element tag="application" type="span" color="a0b0c0">
			<name locale="en">Application</name>
		</element>
 		<element tag="bibliomisc" type="span" color="a0b0c0">
			<name locale="en">Miscellaneous Bibliographic Information</name>
		</element>
		<element tag="citetitle" type="span" color="a0b0c0">
			<name locale="en">Title Citation</name>
		</element>
		<element tag="city" type="span" color="a0b0c0">
			<name locale="en">City</name>
		</element>
		<element tag="edition" type="span" color="a0b0c0">
			<name locale="en">Edition</name>
		</element>
		<element tag="email" type="span" color="a0b0c0">
			<name locale="en">email</name>
			<short-desc locale="en">An email address</short-desc>
		</element>
		<element tag="emphasis" type="span" color="a2b0c0">
			<name locale="en">Emphasis</name>
		</element>
		<element tag="filename" type="span" color="a0b0c0">
			<name locale="en">Filename</name>
		</element>
		<element tag="firstname" type="span" color="a0b0c0">
			<name locale="en">First Name</name>
		</element>
		<element tag="firstterm" type="span" color="a0b0c0">
			<name locale="en">First Term </name>
			<short-desc locale="en">The first place where a word or term occurs in a particular context</short-desc>
		</element>
		<element tag="function" type="span" color="a0b0c0">
			<name locale="en">Function</name>
			<short-desc locale="en">The name of a function, subroutine or procedure</short-desc>
		</element>
		<element tag="literal" type="span" color="a0b0c0">
			<name locale="en">Literal</name>
			<short-desc locale="en">A specific piece of data from a computer system, to be taken literally.</short-desc>
		</element>
		<element tag="primary" type="span" color="a0b0c0">
		</element>
		<element tag="productname" type="span" color="a0b0c0">
			<name locale="en">Product Name</name>
		</element>
		<element tag="replaceable" type="span" color="a0b0c0">
			<name locale="en">Replaceable Text</name>
		</element>
		<element tag="secondary" type="span" color="a0b0c0">
		</element>
		<element tag="sgmltag" type="span" color="a0b0c0">
			<name locale="en">SGML Tag</name>
		</element>
		<element tag="structname" type="span" color="a0b0c0">
			<name locale="en">Structure Name</name>
		</element>
		<element tag="surname" type="span" color="a0b0c0">
			<name locale="en">Surname</name>
		</element>
		<element tag="symbol" type="span" color="a0b0c0">
			<name locale="en">Symbol</name>
		</element>
		<element tag="term" type="span" color="a0b0c0">
			<name locale="en">Term</name>
		</element>
		<element tag="ulink" type="span" color="a0b0c0">
			<name locale="en">URL Link</name>
			<short-desc locale="en">A URL link (perhaps to a web site)</short-desc>
		</element>
		<element tag="xref" type="span" color="a0b0c0">
			<name locale="en">Cross-reference</name>
		</element>


		<element tag="guibutton" type="span" color="a0b0c0">
			<name locale="en">GUI Button</name>
		</element>
		<element tag="guilabel" type="span" color="a0b0c0">
			<name locale="en">GUI Label</name>
		</element>
		<element tag="guimenu" type="span" color="a0b0c0">
			<name locale="en">GUI Menu</name>
		</element>
		<element tag="guisubmenu" type="span" color="a0b0c0">
			<name locale="en">GUI Submenu</name>
		</element>
		<element tag="guimenuitem" type="span" color="a0b0c0">
			<name locale="en">GUI Menu Item</name>
		</element>
		<element tag="keycap" type="span" color="a0b0c0">
			<name locale="en">Key Caption</name>
			<short-desc locale="en">The printed text on a particular key on a keyboard</short-desc>
		</element>
		<element tag="menuchoice" type="span" color="a0b0c0">
			<name locale="en">Menu Choice</name>
		</element>

		<!-- etc -->

	</element-list>

</dispspec>
